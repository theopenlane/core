    "river.Config": {
      "properties": {
        "AdvisoryLockPrefix": {
          "type": "integer"
        },
        "CancelledJobRetentionPeriod": {
          "type": "integer"
        },
        "CompletedJobRetentionPeriod": {
          "type": "integer"
        },
        "DiscardedJobRetentionPeriod": {
          "type": "integer"
        },
        "ErrorHandler": true,
