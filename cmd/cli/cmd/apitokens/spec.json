{
  "name": "api token",
  "use": "token",
  "aliases": ["tokens", "api-token", "api-tokens", "apitoken", "apitokens"],
  "short": "the subcommands for working with api tokens",
  "list": {
    "method": "GetAllAPITokens",
    "root": "apiTokens"
  },
  "get": {
    "method": "GetAPITokenByID",
    "idFlag": {"name": "id", "shorthand": "i", "usage": "api token id to query"},
    "resultPath": ["apiToken"],
    "listRoot": "apiTokens",
    "fallbackList": true
  },
  "create": {
    "method": "CreateAPIToken",
    "inputType": "openlaneclient.CreateAPITokenInput",
    "resultPath": ["createAPIToken", "apiToken"],
    "fields": [
      {"flag": {"name": "name", "shorthand": "n", "usage": "name of the api token", "required": true}, "field": "Name", "kind": "string"},
      {"flag": {"name": "description", "shorthand": "d", "usage": "description of the api token"}, "field": "Description", "kind": "string"},
      {"flag": {"name": "expiration", "shorthand": "e", "usage": "duration the token remains valid"}, "field": "ExpiresAt", "kind": "duration", "parser": "durationFromNow"},
      {"flag": {"name": "scopes", "usage": "scopes to associate with the api token", "default": "read,write"}, "field": "Scopes", "kind": "stringSlice"}
    ]
  },
  "update": {
    "method": "UpdateAPIToken",
    "idFlag": {"name": "id", "shorthand": "i", "usage": "api token id to update"},
    "inputType": "openlaneclient.UpdateAPITokenInput",
    "resultPath": ["updateAPIToken", "apiToken"],
    "fields": [
      {"flag": {"name": "name", "shorthand": "n", "usage": "name of the api token"}, "field": "Name", "kind": "string"},
      {"flag": {"name": "description", "shorthand": "d", "usage": "description of the api token"}, "field": "Description", "kind": "string"},
      {"flag": {"name": "scopes", "usage": "scopes to associate with the api token"}, "field": "Scopes", "kind": "stringSlice"}
    ]
  },
  "delete": {
    "method": "DeleteAPIToken",
    "idFlag": {"name": "id", "shorthand": "i", "usage": "api token id to delete"},
    "resultPath": ["deleteAPIToken", "deletedID"],
    "resultField": "deletedID"
  },
  "columns": [
    {"header": "ID", "path": ["id"]},
    {"header": "Name", "path": ["name"]},
    {"header": "Scopes", "path": ["scopes"], "formatter": "joinedStrings"},
    {"header": "ExpiresAt", "path": ["expiresAt"], "formatter": "timeOrNever"}
  ],
  "deleteColumns": [
    {"header": "DeletedID", "path": ["deletedID"]}
  ]
}
