{
  "name": "personalaccesstokens",
  "use": "pat",
  "short": "the subcommands for working with personal access tokens",
  "list": {
    "method": "GetAllPersonalAccessTokens",
    "root": "personalAccessTokens"
  },
  "get": {
    "method": "GetPersonalAccessTokenByID",
    "idFlag": {
      "name": "id",
      "shorthand": "i",
      "usage": "pat id to query"
    },
    "resultPath": ["personalAccessToken"],
    "fallbackList": true
  },
  "create": {
    "method": "CreatePersonalAccessToken",
    "inputType": "openlaneclient.CreatePersonalAccessTokenInput",
    "resultPath": ["createPersonalAccessToken", "personalAccessToken"],
    "fields": [
      {
        "flag": {
          "name": "name",
          "shorthand": "n",
          "usage": "name of the personal access token",
          "required": true
        },
        "field": "Name",
        "kind": "string"
      },
      {
        "flag": {
          "name": "description",
          "shorthand": "d",
          "usage": "description of the pat"
        },
        "field": "Description",
        "kind": "string"
      },
      {
        "flag": {
          "name": "organizations",
          "shorthand": "o",
          "usage": "organization(s) id to associate the pat with"
        },
        "field": "OrganizationIDs",
        "kind": "stringSlice"
      },
      {
        "flag": {
          "name": "expiration",
          "shorthand": "e",
          "usage": "duration of the pat to be valid, leave empty to never expire"
        },
        "field": "ExpiresAt",
        "kind": "duration",
        "parser": "durationFromNow"
      }
    ]
  },
  "update": {
    "method": "UpdatePersonalAccessToken",
    "idFlag": {
      "name": "id",
      "shorthand": "i",
      "usage": "pat id to update",
      "required": true
    },
    "inputType": "openlaneclient.UpdatePersonalAccessTokenInput",
    "resultPath": ["updatePersonalAccessToken", "personalAccessToken"],
    "fields": [
      {
        "flag": {
          "name": "name",
          "shorthand": "n",
          "usage": "name of the personal access token"
        },
        "field": "Name",
        "kind": "string"
      },
      {
        "flag": {
          "name": "description",
          "shorthand": "d",
          "usage": "description of the pat"
        },
        "field": "Description",
        "kind": "string"
      },
      {
        "flag": {
          "name": "add-organizations",
          "shorthand": "o",
          "usage": "add organization(s) id"
        },
        "field": "AddOrganizationIDs",
        "kind": "stringSlice"
      },
      {
        "flag": {
          "name": "remove-organizations",
          "shorthand": "r",
          "usage": "remove organization(s) id"
        },
        "field": "RemoveOrganizationIDs",
        "kind": "stringSlice"
      }
    ]
  },
  "delete": {
    "method": "DeletePersonalAccessToken",
    "idFlag": {
      "name": "id",
      "shorthand": "i",
      "usage": "pat id to delete",
      "required": true
    },
    "resultPath": ["deletePersonalAccessToken", "deletedID"],
    "resultField": "deletedID"
  },
  "columns": [
    {"header": "ID", "path": ["id"]},
    {"header": "Name", "path": ["name"]},
    {"header": "Token", "path": ["token"]},
    {"header": "Authorized Organizations", "path": ["organizations", "edges"], "formatter": "edgeNames"},
    {"header": "LastUsedAt", "path": ["lastUsedAt"], "formatter": "timeOrNever"},
    {"header": "ExpiresAt", "path": ["expiresAt"], "formatter": "timeOrNever"}
  ],
  "deleteColumns": [
    {"header": "DeletedID", "path": ["deletedID"]}
  ]
}
